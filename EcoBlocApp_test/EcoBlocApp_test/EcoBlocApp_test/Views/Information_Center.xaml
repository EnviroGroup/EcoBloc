<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EcoBlocApp_test.Views.Information_Center"
             Title="Information Center"
             xmlns:local="clr-namespace:EcoBlocApp_test.ViewModels">

    
    
    <ContentPage.Content>
        <StackLayout>
            <Button Text="General Information"
            Command="{Binding GeneralInformationCommand}"/> 
            
            <Button Text="Sustainable Practices"
            Command="{Binding SustainablePracticesCommand}"/>
                                
            <Button Text="Directory"
            Command="{Binding DirectoryCommand}"/>
                               



        </StackLayout>
    </ContentPage.Content>
</ContentPage>