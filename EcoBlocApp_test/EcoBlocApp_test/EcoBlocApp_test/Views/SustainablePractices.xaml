<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EcoBlocApp_test.Views.SustainablePractices"
             Title="Sustainable Practices">

    <ContentPage.Content>
        <StackLayout>
                                <Button Text="Environmental Sustainability"
                                        Command="{Binding EnvironmentalSustainabilityCommand}"/>
                                <Button Text="Recycling"
                                        Command="{Binding RecyclingCommand}"/>

        </StackLayout>
        </ContentPage.Content>

</ContentPage>